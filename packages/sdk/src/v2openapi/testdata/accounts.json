      "type": "object",
      "properties": {
        "aid": {
          "description": "Account ID",
          "type": "string"
        },
        "created_at": {
          "description": "unix time of when the account was created",
          "type": "integer"
        },
        "profile": {
          "$ref": "#/definitions/types_UserProfile"
        },
